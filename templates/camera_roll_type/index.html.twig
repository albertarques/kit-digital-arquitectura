{% extends 'base.html.twig' %}

{% block title %}CameraRollType index{% endblock %}

{% block body %}
    <h1>CameraRollType index</h1>

    <table class="table">
        <thead>
            <tr>
                <th>Id</th>
                <th>Created</th>
                <th>Updated</th>
                <th>actions</th>
            </tr>
        </thead>
        <tbody>
        {% for camera_roll_type in camera_roll_types %}
            <tr>
                <td>{{ camera_roll_type.id }}</td>
                <td>{{ camera_roll_type.created ? camera_roll_type.created|date('Y-m-d H:i:s') : '' }}</td>
                <td>{{ camera_roll_type.updated ? camera_roll_type.updated|date('Y-m-d H:i:s') : '' }}</td>
                <td>
                    <a href="{{ path('app_camera_roll_type_show', {'id': camera_roll_type.id}) }}">show</a>
                    <a href="{{ path('app_camera_roll_type_edit', {'id': camera_roll_type.id}) }}">edit</a>
                </td>
            </tr>
        {% else %}
            <tr>
                <td colspan="4">no records found</td>
            </tr>
        {% endfor %}
        </tbody>
    </table>

    <a href="{{ path('app_camera_roll_type_new') }}">Create new</a>
{% endblock %}
